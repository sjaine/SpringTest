<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="design.jaine.spring.test.mvc.repository.RealtorRepository">
 
 	<insert id="insertRealtor" parameterType="design.jaine.spring.test.mvc.domain.Realtor"
 	useGeneratedKeys="true" keyProperty="id">
 		INSERT INTO
 			`office`
 		(
 			`office`
 			, `phoneNumber`
 			, `address`
 			, `grade`
 		)
 		VALUE
 		(
 			#{office}
 			, #{phoneNumber}
 			, #{address}
 			, #{grade}
 		)
 	</insert>
 
 </mapper>